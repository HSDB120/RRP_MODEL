<?xml version="1.0"?>
<package format="3">
  <name>ms2r1p</name>
  <version>0.0.2</version>
  <description>SCARA/MS2R1P manipulator package (refactored custom version)</description>
  <maintainer email="tuemail@dominio.com">Herick Steven Duran Burgos</maintainer>
  <license>Apache-2.0</license>

  <depend>rclpy</depend>
  <depend>geometry_msgs</depend>
  <depend>sensor_msgs</depend>
